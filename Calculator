x = float(input('Number One='))
y = float(input('Number Two='))
operation = input('operation:')
result = 0

if operation == '+':
    result = x + y
elif operation == '-':
    result = x - y
elif operation == '*':
    result = x * y
elif operation == '/':
    result = x / y
else:
    print('Error')

if result is not None:
    print('Result:', result)
